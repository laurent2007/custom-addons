/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.grey_12 {
  font-size: 12px;
  color: #999;
  padding-right: 40rpx;
  align-self: center;
} 

.normal_font{
  font-size: 14px;
  letter-spacing:0.3ch;
}


.btn_redline{
  border: 1px solid #ff6666;
  color: #ff6666;
  width: 380rpx;
  background-color: transparent;
}

.location{
  align-content: center;
  align-self: center;

}

.empty{
  flex: 1;
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  background: #fff;
  font-size: 12px;
  padding: 0 0 10rpx 20rpx;
  height: 36rpx;

}

.userinfo-avatar {
  width: 98rpx;
  height: 98rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
  font-size: 14px;
}

.header {
  flex: 1;
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  background: #fff;
  font-size: 12px;
  padding: 0 0 10rpx 20rpx;
  height: 36rpx;
}


.title {
  font-size: 14px;
  margin-left: 24rpx;
}