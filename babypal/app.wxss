.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.grey_12 {
  font-size: 12px;
  color: #999;
  padding-right: 40rpx;
  align-self: center;
} 

.location{
  align-content: center;
  align-self: center;

}

.empty{
  flex: 1;
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  background: #fff;
  font-size: 12px;
  padding: 0 0 10rpx 20rpx;
  height: 36rpx;

}